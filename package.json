{
  "name": "project-form",
  "version": "1.0.0",
  "type": "module",
  "description": "A simple project to learn web development",
  "source": "src/index.html",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": "~16.14.2"
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js ./src",
    "test:cov": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage ./src",
    "lint": "eslint ./src",
    "test:e2e": "npx cypress run --browser chrome",
    "prepare": "husky install"
  },
  "staticFiles": {
    "staticPath": "./src/static"
  },
  "keywords": [],
  "author": "Taissa Freire",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@jest/globals": "^29.5.0",
    "@parcel/packager-raw-url": "^2.8.2",
    "@parcel/transformer-webmanifest": "^2.8.2",
    "babel-jest": "^29.5.0",
    "cypress": "^12.7.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "parcel": "^2.8.2",
    "parcel-reporter-static-files-copy": "^1.5.0",
    "prettier": "2.8.4"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "^1.8.3",
    "pikaday": "^1.8.2"
  }
}
